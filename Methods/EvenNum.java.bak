class EvenNum
{
	public static void EvenOdd (int a )
	{
		if (a%2==0)
		{
			System.out.println(a+ " is a even number");
		}
		else
		{
			System.out.println(a+" is a odd number");
		}
	}
	public static void main(String[] args) 
	{
		System.out.println("Hello World!");
		EvenOdd(12);
		EvenOdd(100);
	}
}